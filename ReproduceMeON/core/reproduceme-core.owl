    <!-- https://w3id.org/reproduceme-core#hasExperimentMaterial -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#hasExperimentMaterial">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasInputVar"/>
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#ExperimentMaterial"/>
        <rdfs:comment xml:lang="en">hasExperimentalMaterial binds an experiment to the data that was used in an experiment.</rdfs:comment>
        <rdfs:label>hasExperimentalMaterial</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#hasExperimentalCondition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#hasExperimentalCondition">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasInputVar"/>
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#ExperimentalCondition"/>
        <rdfs:comment xml:lang="en">hasExperimentalCondition binds an experiment to its environmental condition.</rdfs:comment>
        <rdfs:label xml:lang="en">hasExperimentalCondition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#hasExperimentalData -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#hasExperimentalData">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/net/p-plan#hasInputVar"/>
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#ExperimentalData"/>
        <rdfs:comment xml:lang="en">hasExperimentalData binds an experiment to the data that was used in an experiment.</rdfs:comment>
        <rdfs:label>hasExperimentalData</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#hasPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#hasPart">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="https://w3id.org/reproduceme-core#isPartOf"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#Instrument"/>
        <rdfs:comment xml:lang="en">An entity is part of another entity if one of its part belong to the other entity.</rdfs:comment>
        <rdfs:label xml:lang="en">hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#hasSetting -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#hasSetting">
        <owl:inverseOf rdf:resource="https://w3id.org/reproduceme-core#isSettingOf"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#Setting"/>
        <rdfs:comment xml:lang="en">hasSetting binds a thing or instrument to its configurations or settings.</rdfs:comment>
        <rdfs:label xml:lang="en">hasSetting</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#isAccessibleTo -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#isAccessibleTo">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">isAccessibleTo is a property which describes that an Entity is accessible or available to an Agent.</rdfs:comment>
        <rdfs:label xml:lang="en">isAccessibleTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#isPartOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#isPartOf">
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Instrument"/>
        <rdfs:comment xml:lang="en">An entity is part of another entity if one of its part belong to the other entity.</rdfs:comment>
        <rdfs:label xml:lang="en">isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#isSettingOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#isSettingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Setting"/>
        <rdfs:comment xml:lang="en">isSettingOf binds a thing or instrument to its configurations or settings.</rdfs:comment>
        <rdfs:label xml:lang="en">isSettingOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#reference -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#reference">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">An entity which has used another entity as reference.</rdfs:comment>
        <rdfs:label xml:lang="en">reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#storedAt -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#storedAt">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#atLocation"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Location"/>
        <rdfs:comment xml:lang="en">The storage location of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">storedAt</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#usedMethod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#usedMethod">
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#Method"/>
        <rdfs:comment xml:lang="en">A process which used a method to achieve something.</rdfs:comment>
        <rdfs:label xml:lang="en">usedMethod</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#wasCreatedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#wasCreatedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasAttributedTo"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">A relationship between an entity to its creator agent.</rdfs:comment>
        <rdfs:label xml:lang="en">wasCreatedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#wasDistributedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#wasDistributedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasAttributedTo"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">A relationship between an entity to its distributor agent.</rdfs:comment>
        <rdfs:label xml:lang="en">wasDistributedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#wasExtractedFrom -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#wasExtractedFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#ExperimentMaterial"/>
        <rdfs:range rdf:resource="https://w3id.org/reproduceme-core#ExperimentMaterial"/>
        <rdfs:comment xml:lang="en">An experiment material which is extracted from another material.</rdfs:comment>
        <rdfs:label xml:lang="en">wasExtractedFrom</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#wasPerformedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#wasPerformedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/reproduceme-core#wasUpdatedBy -->

    <owl:ObjectProperty rdf:about="https://w3id.org/reproduceme-core#wasUpdatedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:comment xml:lang="en">An entity which is updated by another entity or an agent.</rdfs:comment>
        <rdfs:label xml:lang="en">wasUpdatedBy</rdfs:label>
    </owl:ObjectProperty>
    


