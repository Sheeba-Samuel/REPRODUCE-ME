        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Publication"/>
        <rdfs:comment>Publication persistent identifier</rdfs:comment>
        <rdfs:label>doi</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#id -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#id">
        <rdfs:comment xml:lang="en">A unique identifier of an entity.</rdfs:comment>
        <rdfs:label xml:lang="en">Id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#modifiedAtTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#modifiedAtTime">
        <rdfs:domain rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The time at which an entity was modified.</rdfs:comment>
        <rdfs:label>modifiedAtTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#name -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#name">
        <rdfs:comment xml:lang="en">The name of a thing.</rdfs:comment>
        <rdfs:label xml:lang="en">Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#pmcid -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#pmcid">
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Publication"/>
        <rdfs:comment xml:lang="en">A standard unique PubMed Central identifier.</rdfs:comment>
        <rdfs:label>PubMed Central ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#pubmedid -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#pubmedid">
        <rdfs:domain rdf:resource="https://w3id.org/reproduceme-core#Publication"/>
        <rdfs:comment xml:lang="en">A standard unique identifier for each PubMed record.</rdfs:comment>
        <rdfs:label>PubMed ID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#receivedAtTime -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#receivedAtTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>The time at which an entity was received.</rdfs:comment>
        <rdfs:label>receivedAtTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/reproduceme-core#Algorithm -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#Algorithm">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Plan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/net/p-plan#isSubPlanOfPlan"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <opla:isNativeTo rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:comment xml:lang="en">A set of rules or steps to be followed in a problem-solving operation.</rdfs:comment>
        <rdfs:label xml:lang="en">Algorithm</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#Annotation -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#Annotation">
        <rdfs:subClassOf rdf:resource="https://w3id.org/reproduceme-core#ExperimentalData"/>
        <rdfs:comment xml:lang="en">A description added to a thing which provides comment, note or description.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#Author -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#Author">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#address"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#name"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#ORCID"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <opla:isNativeTo rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">The person who authors a piece of work.</rdfs:comment>
        <rdfs:label xml:lang="en">Author</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#CausalEffect -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#CausalEffect">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Variable"/>
        <opla:isNativeTo rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:comment xml:lang="en">The causal effects of an activity denotes the effects on an outcome because of another activity.</rdfs:comment>
        <rdfs:label xml:lang="en">Causal Effect</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#ComputationalStep -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#ComputationalStep">
        <rdfs:subClassOf rdf:resource="http://purl.org/net/p-plan#Step"/>
        <opla:isNativeTo rdf:resource="https://w3id.org/reproduceme-core#Experiment"/>
        <rdfs:comment xml:lang="en">The step or the process which uses computational resources involved in an experiment.</rdfs:comment>
        <rdfs:label xml:lang="en">Computational Step</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#Configuration -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#Configuration">
        <rdfs:subClassOf rdf:resource="https://w3id.org/reproduceme-core#Setting"/>
        <rdfs:comment xml:lang="en">The settings or changes applied to an instrument.</rdfs:comment>
        <rdfs:label xml:lang="en">Configuration</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#ContactPerson -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#ContactPerson">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#address"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#name"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#ORCID"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <opla:isNativeTo rdf:resource="http://www.w3.org/ns/prov#Agent"/>
        <rdfs:comment xml:lang="en">A person who has the role of being a contact person for a certain thing.</rdfs:comment>
        <rdfs:label xml:lang="en">ContactPerson</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#Context -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#Context">
        <rdfs:subClassOf rdf:resource="https://w3id.org/reproduceme-core#Setting"/>
        <rdfs:comment xml:lang="en">The background setting of an experiment.</rdfs:comment>
        <rdfs:label xml:lang="en">Context</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/reproduceme-core#CopyrightHolder -->

    <owl:Class rdf:about="https://w3id.org/reproduceme-core#CopyrightHolder">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#address"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/reproduceme-core#name"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
