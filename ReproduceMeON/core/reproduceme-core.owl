        <rdfs:comment xml:lang="en">A standard alphanumeric code to uniquely identify scientific and other academic authors and contributors.</rdfs:comment>
        <rdfs:label>ORCID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#address -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#address">
        <rdfs:comment>Address of an Agent</rdfs:comment>
        <rdfs:label>Address</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/reproduceme-core#description -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/reproduceme-core#description">
