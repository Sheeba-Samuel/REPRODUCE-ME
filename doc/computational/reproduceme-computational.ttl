                                              rdfs:label "The ReproduceMe-Computational Ontology" .

#################################################################
#    Data properties
#################################################################

###  https://w3id.org/reproduceme/computational#cellType
:cellType rdf:type owl:DatatypeProperty .


###  https://w3id.org/reproduceme/computational#executionTime
:executionTime rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/reproduceme/computational#Argument
:Argument rdf:type owl:Class ;
          rdfs:subClassOf <https://w3id.org/reproduceme/core#ExperimentalData> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://purl.org/net/p-plan#isInputVarOf> ;
                            owl:someValuesFrom :FunctionActivation
                          ] ;
          <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
          rdfs:comment """A parameter.
For example, an argument of parameter of a function."""@en ;
          rdfs:label "Argument"@en .


###  https://w3id.org/reproduceme/computational#Cell
:Cell rdf:type owl:Class ;
      rdfs:subClassOf <https://w3id.org/reproduceme/core#ComputationalStep> ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://purl.org/net/p-plan#isPrecededBy> ;
                        owl:someValuesFrom :Cell
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://purl.org/net/p-plan#isPrecededBy> ;
                        owl:allValuesFrom :Cell
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://purl.org/net/p-plan#isStepOfPlan> ;
                        owl:allValuesFrom :Notebook
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://purl.org/net/p-plan#hasInputVar> ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Source
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty <http://purl.org/net/p-plan#isStepOfPlan> ;
                        owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onClass :Notebook
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :cellType ;
                        owl:allValuesFrom xsd:string
                      ] ;
      <http://ontologydesignpatterns.org/opla#isNativeTo> :Notebook ;
      rdfs:comment """A cell is a multiline text input field in an Interactive Notebook which can either be code cells where code can be excuted or markdown cells where a descriptive text can be provided or raw cells where output can directly be entered. 
For example, a Jupyter Notebook cell."""@en ;
      rdfs:label "Notebok Cell"@en .


###  https://w3id.org/reproduceme/computational#CellExecution
:CellExecution rdf:type owl:Class ;
               rdfs:subClassOf <http://purl.org/net/p-plan#Activity> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/ns/prov#generated> ;
                                 owl:someValuesFrom <http://www.w3.org/ns/prov#Entity>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/ns/prov#used> ;
                                 owl:someValuesFrom <http://www.w3.org/ns/prov#Entity>
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://purl.org/net/p-plan#correspondsToStep> ;
                                 owl:allValuesFrom :Cell
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://purl.org/net/p-plan#correspondsToStep> ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Cell
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :executionTime ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:dateTime
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:dateTime
                               ] ;
               <http://ontologydesignpatterns.org/opla#isNativeTo> :Notebook ;
               rdfs:comment "The run of a cell of a Interactive Notebook."@en ;
               rdfs:label "Notebook Cell Execution"@en .


###  https://w3id.org/reproduceme/computational#ComputationalTool
:ComputationalTool rdf:type owl:Class ;
                   rdfs:subClassOf <https://w3id.org/reproduceme/core#Setting> ;
                   rdfs:comment "The tools that use computer-based systems for computation."@en ;
                   rdfs:label "Computational Tool"@en .


###  https://w3id.org/reproduceme/computational#Function
:Function rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                            owl:someValuesFrom :Script
                          ] ;
          <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
          rdfs:comment "A programming language code snippet in a script which describes a procedure or a routine."@en ;
          rdfs:label "Function"@en .


###  https://w3id.org/reproduceme/computational#FunctionActivation
:FunctionActivation rdf:type owl:Class ;
                    rdfs:subClassOf <https://w3id.org/reproduceme/core#ComputationalStep> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://purl.org/net/p-plan#hasInputVar> ;
                                      owl:someValuesFrom :Argument
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://purl.org/net/p-plan#hasOutputVar> ;
                                      owl:someValuesFrom <https://w3id.org/reproduceme/core#Output>
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty <http://purl.org/net/p-plan#isStepOfPlan> ;
                                      owl:allValuesFrom :Script
                                    ] ;
                    <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
                    rdfs:comment "Function Activation is when a function is activated or run."@en ;
                    rdfs:label "Function Activation"@en .


###  https://w3id.org/reproduceme/computational#Kernel
:Kernel rdf:type owl:Class ;
        rdfs:subClassOf <https://w3id.org/reproduceme/core#Setting> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <https://w3id.org/reproduceme/core#name> ;
                          owl:allValuesFrom xsd:string
                        ] ;
        rdfs:comment "An Interactive Notebook computation engine that executes the code written in a programming language."@en ;
        rdfs:label "Kernel"@en .


###  https://w3id.org/reproduceme/computational#Library
:Library rdf:type owl:Class ;
         rdfs:subClassOf <https://w3id.org/reproduceme/core#ExperimentalData> ;
         <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
         rdfs:comment "A collection of resources used for the development of software." ;
         rdfs:label "Library" .


###  https://w3id.org/reproduceme/computational#Module
:Module rdf:type owl:Class ;
        rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ;
        <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
        rdfs:comment "A part of a computer program or a software."@en ;
        rdfs:label "Module"@en .


###  https://w3id.org/reproduceme/computational#Notebook
:Notebook rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                          <https://w3id.org/reproduceme/core#ExperimentalData> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                            owl:someValuesFrom <https://w3id.org/reproduceme/core#Experiment>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/reproduceme/core#hasSetting> ;
                            owl:someValuesFrom :Kernel
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/reproduceme/core#hasSetting> ;
                            owl:someValuesFrom :OperatingSystem
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <https://w3id.org/reproduceme/core#hasSetting> ;
                            owl:someValuesFrom :ProgrammingLanguage
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
                            owl:allValuesFrom <http://www.w3.org/ns/prov#Agent>
                          ] ;
          <http://ontologydesignpatterns.org/opla#isNativeTo> <https://w3id.org/reproduceme/core#Experiment> ;
          rdfs:comment """Interactive Notebook is a notebook for interactive computation and allow users to run code.
For example, Jupyter Notebook"""@en ;
          rdfs:label "Notebok"@en .


###  https://w3id.org/reproduceme/computational#OperatingSystem
:OperatingSystem rdf:type owl:Class ;
                 rdfs:subClassOf <https://w3id.org/reproduceme/core#Setting> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty <https://w3id.org/reproduceme/core#name> ;
                                   owl:allValuesFrom xsd:string
                                 ] ;
                 rdfs:comment "A system software that supports and manages the hardware and the software resources and provides computer's functions."@en ;
                 rdfs:label "Operating System"@en .


###  https://w3id.org/reproduceme/computational#Package
:Package rdf:type owl:Class ;
         rdfs:subClassOf <https://w3id.org/reproduceme/core#Setting> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty <https://w3id.org/reproduceme/core#name> ;
                           owl:allValuesFrom xsd:string
                         ] ;
         rdfs:comment "A collection of programs and resources which are packed together."@en ;
         rdfs:label "Package"@en .


###  https://w3id.org/reproduceme/computational#Parameter
:Parameter rdf:type owl:Class ;
           rdfs:subClassOf <https://w3id.org/reproduceme/core#ExperimentalData> ;
           <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
           rdfs:comment "These values are factors which define an operation or a system which is kept constant for a particular execution of an experiment or a calculation and varied over other executions."@en ;
           rdfs:label "Parameter"@en .


###  https://w3id.org/reproduceme/computational#ProgrammingLanguage
:ProgrammingLanguage rdf:type owl:Class ;
                     rdfs:subClassOf <https://w3id.org/reproduceme/core#Setting> ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty <https://w3id.org/reproduceme/core#name> ;
                                       owl:allValuesFrom xsd:string
                                     ] ;
                     rdfs:comment "The language used for scripting. For eg. Java, Python."@en ;
                     rdfs:label "Programming Language"@en .


###  https://w3id.org/reproduceme/computational#Script
:Script rdf:type owl:Class ;
        rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                        <https://w3id.org/reproduceme/core#ExperimentalData> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                          owl:someValuesFrom <https://w3id.org/reproduceme/core#Experiment>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
                          owl:allValuesFrom <http://www.w3.org/ns/prov#Agent>
                        ] ;
        <http://ontologydesignpatterns.org/opla#isNativeTo> <https://w3id.org/reproduceme/core#Experiment> ;
        rdfs:comment "A computer program written to perform a task in a scripting language."@en ;
        rdfs:label "Script"@en .


###  https://w3id.org/reproduceme/computational#Source
:Source rdf:type owl:Class ;
        rdfs:subClassOf <https://w3id.org/reproduceme/core#ExperimentalData> ;
        <http://ontologydesignpatterns.org/opla#isNativeTo> :Notebook ;
        rdfs:comment "The input of each cell in a Jupyter Notebook is described as Source."@en ;
        rdfs:label "Source"@en .


###  https://w3id.org/reproduceme/computational#Trial
:Trial rdf:type owl:Class ;
       rdfs:subClassOf <http://purl.org/net/p-plan#Activity> ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#used> ;
                         owl:someValuesFrom :Library
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#used> ;
                         owl:someValuesFrom :Module
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#used> ;
                         owl:someValuesFrom :OperatingSystem
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#used> ;
                         owl:someValuesFrom :ProgrammingLanguage
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://purl.org/net/p-plan#correspondsToStep> ;
                         owl:allValuesFrom :FunctionActivation
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#atLocation> ;
                         owl:allValuesFrom <http://www.w3.org/ns/prov#Location>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :executionTime ;
                         owl:allValuesFrom xsd:string
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:dateTime
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onDataRange xsd:dateTime
                       ] ;
       <http://ontologydesignpatterns.org/opla#isNativeTo> :Script ;
       rdfs:comment """A run or execution of an activity.
For example, one execution of a script."""@en ;
       rdfs:label "Trial"@en .


