@prefix : <https://w3id.org/reproduceme/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/reproduceme/core> .

<https://w3id.org/reproduceme/core> rdf:type owl:Ontology ;
                                     owl:imports <http://purl.org/net/p-plan#> ,
                                                 <http://www.w3.org/ns/prov-o-20130430> ;
                                     <http://purl.org/dc/terms/created> "2020-07-28"@en ;
                                     <http://purl.org/dc/terms/creator> "Sheeba Samuel"@en ,
                                                                        "Birgitta KÃ¶nig-Ries" ;
                                     <http://purl.org/dc/terms/description> """The ReproduceMe-Core ontology is the core ontology of the ReproduceMe Ontology Network. The ReproduceMe-Core ontology is designed to provide a detailed abstract definition of structured knowledge for the provenance and reproducibility of scientific studies.
It is based on the foundational ontologies: PROV-O and P-Plan. It is an extension of the PROV-O and the P-Plan ontology to describe a complete path of a scientific experiment. It expresses the REPRODUCE-ME Data Model using the OWL2 Web Ontology Language (OWL2). It provides a set of classes and properties to represent a scientific experiment including its computational and non-computational steps to track the provenance of results. It describes an experiment and its data, agents, activities, plans, steps, variables, instruments, materials and settings required for its reproducibility."""@en ;
                                     <http://purl.org/dc/terms/license> "http://creativecommons.org/licenses/by-nc-sa/2.0/" ;
                                     <http://purl.org/dc/terms/modified> "2020-07-28"@en ;
                                     <http://purl.org/dc/terms/title> "The ReproduceMe-Core ontology" ;
                                     <http://purl.org/vocab/vann/preferredNamespacePrefix> "repr-core"@en ;
                                     rdfs:label "The ReproduceMe-Core ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://ontologydesignpatterns.org/opla#isNativeTo
<http://ontologydesignpatterns.org/opla#isNativeTo> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/reproduceme/core#hasExperimentMaterial
:hasExperimentMaterial rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://purl.org/net/p-plan#hasInputVar> ;
                       rdfs:domain :Experiment ;
                       rdfs:range :ExperimentMaterial ;
                       rdfs:comment "hasExperimentalMaterial binds an experiment to the data that was used in an experiment."@en ;
                       rdfs:label "hasExperimentalMaterial" .


###  https://w3id.org/reproduceme/core#hasExperimentalCondition
:hasExperimentalCondition rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://purl.org/net/p-plan#hasInputVar> ;
                          rdfs:domain :Experiment ;
                          rdfs:range :ExperimentalCondition ;
                          rdfs:comment "hasExperimentalCondition binds an experiment to its environmental condition."@en ;
                          rdfs:label "hasExperimentalCondition"@en .


###  https://w3id.org/reproduceme/core#hasExperimentalData
:hasExperimentalData rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://purl.org/net/p-plan#hasInputVar> ;
                     rdfs:domain :Experiment ;
                     rdfs:range :ExperimentalData ;
                     rdfs:comment "hasExperimentalData binds an experiment to the data that was used in an experiment."@en ;
                     rdfs:label "hasExperimentalData" .


###  https://w3id.org/reproduceme/core#hasPart
:hasPart rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf :isPartOf ;
         rdfs:range :Instrument ;
         rdfs:comment "An entity is part of another entity if one of its part belong to the other entity."@en ;
         rdfs:label "hasPart"@en .


###  https://w3id.org/reproduceme/core#hasSetting
:hasSetting rdf:type owl:ObjectProperty ;
            owl:inverseOf :isSettingOf ;
            rdfs:range :Setting ;
            rdfs:comment "hasSetting binds a thing or instrument to its configurations or settings."@en ;
            rdfs:label "hasSetting"@en .


###  https://w3id.org/reproduceme/core#isAccessibleTo
:isAccessibleTo rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasInfluencedBy> ;
                rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
                rdfs:range <http://www.w3.org/ns/prov#Agent> ;
                rdfs:comment "isAccessibleTo is a property which describes that an Entity is accessible or available to an Agent."@en ;
                rdfs:label "isAccessibleTo"@en .


###  https://w3id.org/reproduceme/core#isPartOf
:isPartOf rdf:type owl:ObjectProperty ;
          rdfs:domain :Instrument ;
          rdfs:comment "An entity is part of another entity if one of its part belong to the other entity."@en ;
          rdfs:label "isPartOf"@en .


###  https://w3id.org/reproduceme/core#isSettingOf
:isSettingOf rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Setting ;
             rdfs:comment "isSettingOf binds a thing or instrument to its configurations or settings."@en ;
             rdfs:label "isSettingOf"@en .


###  https://w3id.org/reproduceme/core#reference
:reference rdf:type owl:ObjectProperty ;
           rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
           rdfs:range <http://www.w3.org/ns/prov#Entity> ;
           rdfs:comment "An entity which has used another entity as reference."@en ;
           rdfs:label "reference"@en .


###  https://w3id.org/reproduceme/core#storedAt
:storedAt rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://www.w3.org/ns/prov#atLocation> ;
          rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
          rdfs:range <http://www.w3.org/ns/prov#Location> ;
          rdfs:comment "The storage location of an entity."@en ;
          rdfs:label "storedAt"@en .


###  https://w3id.org/reproduceme/core#usedMethod
:usedMethod rdf:type owl:ObjectProperty ;
            rdfs:range :Method ;
            rdfs:comment "A process which used a method to achieve something."@en ;
            rdfs:label "usedMethod"@en .


###  https://w3id.org/reproduceme/core#wasCreatedBy
:wasCreatedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAttributedTo> ;
              rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
              rdfs:range <http://www.w3.org/ns/prov#Agent> ;
              rdfs:comment "A relationship between an entity to its creator agent."@en ;
              rdfs:label "wasCreatedBy"@en .


###  https://w3id.org/reproduceme/core#wasDistributedBy
:wasDistributedBy rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasAttributedTo> ;
                  rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
                  rdfs:range <http://www.w3.org/ns/prov#Agent> ;
                  rdfs:comment "A relationship between an entity to its distributor agent."@en ;
                  rdfs:label "wasDistributedBy"@en .


###  https://w3id.org/reproduceme/core#wasExtractedFrom
:wasExtractedFrom rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasDerivedFrom> ;
                  rdfs:domain :ExperimentMaterial ;
                  rdfs:range :ExperimentMaterial ;
                  rdfs:comment "An experiment material which is extracted from another material."@en ;
                  rdfs:label "wasExtractedFrom"@en .


###  https://w3id.org/reproduceme/core#wasPerformedBy
:wasPerformedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasInfluencedBy> .


###  https://w3id.org/reproduceme/core#wasUpdatedBy
:wasUpdatedBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf <http://www.w3.org/ns/prov#wasGeneratedBy> ;
              rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
              rdfs:range <http://www.w3.org/ns/prov#Agent> ,
                         <http://www.w3.org/ns/prov#Entity> ;
              rdfs:comment "An entity which is updated by another entity or an agent."@en ;
              rdfs:label "wasUpdatedBy"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/reproduceme/core#ORCID
:ORCID rdf:type owl:DatatypeProperty ;
       rdfs:domain <http://www.w3.org/ns/prov#Agent> ;
       rdfs:comment "A standard alphanumeric code to uniquely identify scientific and other academic authors and contributors."@en ;
       rdfs:label "ORCID" .


###  https://w3id.org/reproduceme/core#address
:address rdf:type owl:DatatypeProperty ;
         rdfs:comment "Address of an Agent" ;
         rdfs:label "Address" .


###  https://w3id.org/reproduceme/core#description
:description rdf:type owl:DatatypeProperty ;
             rdfs:comment "Textual note of an entity." ;
             rdfs:label "description" .


###  https://w3id.org/reproduceme/core#doi
:doi rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf owl:topDataProperty ;
     rdfs:domain :Publication ;
     rdfs:comment "Publication persistent identifier" ;
     rdfs:label "doi" .


###  https://w3id.org/reproduceme/core#id
:id rdf:type owl:DatatypeProperty ;
    rdfs:comment "A unique identifier of an entity."@en ;
    rdfs:label "Id"@en .


###  https://w3id.org/reproduceme/core#modifiedAtTime
:modifiedAtTime rdf:type owl:DatatypeProperty ;
                rdfs:domain <http://www.w3.org/ns/prov#Entity> ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "The time at which an entity was modified." ;
                rdfs:label "modifiedAtTime" .


###  https://w3id.org/reproduceme/core#name
:name rdf:type owl:DatatypeProperty ;
      rdfs:comment "The name of a thing."@en ;
      rdfs:label "Name"@en .


###  https://w3id.org/reproduceme/core#pmcid
:pmcid rdf:type owl:DatatypeProperty ;
       rdfs:domain :Publication ;
       rdfs:comment "A standard unique PubMed Central identifier."@en ;
       rdfs:label "PubMed Central ID" .


###  https://w3id.org/reproduceme/core#pubmedid
:pubmedid rdf:type owl:DatatypeProperty ;
          rdfs:domain :Publication ;
          rdfs:comment "A standard unique identifier for each PubMed record."@en ;
          rdfs:label "PubMed ID" .


###  https://w3id.org/reproduceme/core#receivedAtTime
:receivedAtTime rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "The time at which an entity was received." ;
                rdfs:label "receivedAtTime" .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/reproduceme/core#Algorithm
:Algorithm rdf:type owl:Class ;
           rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                             owl:someValuesFrom :Experiment
                           ] ;
           <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
           rdfs:comment "A set of rules or steps to be followed in a problem-solving operation."@en ;
           rdfs:label "Algorithm"@en .


###  https://w3id.org/reproduceme/core#Annotation
:Annotation rdf:type owl:Class ;
            rdfs:subClassOf :ExperimentalData ;
            rdfs:comment "A description added to a thing which provides comment, note or description."@en ;
            rdfs:label "Annotation"@en .


###  https://w3id.org/reproduceme/core#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf <http://www.w3.org/ns/prov#Person> ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :address ;
                          owl:allValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :name ;
                          owl:allValuesFrom xsd:string
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :ORCID ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        <http://ontologydesignpatterns.org/opla#isNativeTo> <http://www.w3.org/ns/prov#Agent> ;
        rdfs:comment "The person who authors a piece of work."@en ;
        rdfs:label "Author"@en .


###  https://w3id.org/reproduceme/core#CausalEffect
:CausalEffect rdf:type owl:Class ;
              rdfs:subClassOf <http://purl.org/net/p-plan#Variable> ;
              <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
              rdfs:comment "The causal effects of an activity denotes the effects on an outcome because of another activity."@en ;
              rdfs:label "Causal Effect"@en .


###  https://w3id.org/reproduceme/core#ComputationalStep
:ComputationalStep rdf:type owl:Class ;
                   rdfs:subClassOf <http://purl.org/net/p-plan#Step> ;
                   <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                   rdfs:comment "The step or the process which uses computational resources involved in an experiment."@en ;
                   rdfs:label "Computational Step"@en .


###  https://w3id.org/reproduceme/core#Configuration
:Configuration rdf:type owl:Class ;
               rdfs:subClassOf :Setting ;
               rdfs:comment "The settings or changes applied to an instrument."@en ;
               rdfs:label "Configuration"@en .


###  https://w3id.org/reproduceme/core#ContactPerson
:ContactPerson rdf:type owl:Class ;
               rdfs:subClassOf <http://www.w3.org/ns/prov#Person> ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :address ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :name ;
                                 owl:allValuesFrom xsd:string
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :ORCID ;
                                 owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ;
               <http://ontologydesignpatterns.org/opla#isNativeTo> <http://www.w3.org/ns/prov#Agent> ;
               rdfs:comment "A person who has the role of being a contact person for a certain thing."@en ;
               rdfs:label "ContactPerson"@en .


###  https://w3id.org/reproduceme/core#Context
:Context rdf:type owl:Class ;
         rdfs:subClassOf :Setting ;
         rdfs:comment "The background setting of an experiment."@en ;
         rdfs:label "Context"@en .


###  https://w3id.org/reproduceme/core#CopyrightHolder
:CopyrightHolder rdf:type owl:Class ;
                 rdfs:subClassOf <http://www.w3.org/ns/prov#Person> ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :address ;
                                   owl:allValuesFrom xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :name ;
                                   owl:allValuesFrom xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :ORCID ;
                                   owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onDataRange xsd:string
                                 ] ;
                 <http://ontologydesignpatterns.org/opla#isNativeTo> <http://www.w3.org/ns/prov#Agent> ;
                 rdfs:comment "The person who holds the copyright or the permission of an entity or an activity."@en ;
                 rdfs:label "Copyright Holder"@en .


###  https://w3id.org/reproduceme/core#DataType
:DataType rdf:type owl:Class ;
          rdfs:subClassOf <http://purl.org/net/p-plan#Variable> ;
          <http://ontologydesignpatterns.org/opla#isNativeTo> :ExperimentalData ;
          rdfs:comment "A type or format of a thing."@en ;
          rdfs:label "DataType"@en .


###  https://w3id.org/reproduceme/core#Description
:Description rdf:type owl:Class ;
             rdfs:subClassOf :ExperimentalData ;
             rdfs:comment "The text which is used to provide information about a thing."@en ;
             rdfs:label "Description"@en .


###  https://w3id.org/reproduceme/core#EnvironmentAttribute
:EnvironmentAttribute rdf:type owl:Class ;
                      rdfs:subClassOf :ExperimentalCondition ;
                      rdfs:comment "The attributes of the experimental execution environment."@en ;
                      rdfs:label "Environment Attribute"@en .


###  https://w3id.org/reproduceme/core#EnvironmentSetup
:EnvironmentSetup rdf:type owl:Class ;
                  rdfs:subClassOf :ExperimentSetup ;
                  rdfs:comment "To setup the environment to perform an experiment."@en ;
                  rdfs:label "Environment Setup"@en .


###  https://w3id.org/reproduceme/core#EnvironmentSetupSetting
:EnvironmentSetupSetting rdf:type owl:Class ;
                         rdfs:subClassOf :Setting ;
                         rdfs:comment "The various attribute needed to setup the environment for an experiment."@en ;
                         rdfs:label "Environment Setup Setting"@en .


###  https://w3id.org/reproduceme/core#Experiment
:Experiment rdf:type owl:Class ;
            rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://purl.org/net/p-plan#correspondsToVariable> ;
                              owl:someValuesFrom :ExperimentalData
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                              owl:someValuesFrom :Study
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
                              owl:allValuesFrom <http://www.w3.org/ns/prov#Agent>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasExperimentMaterial ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass :ExperimentMaterial
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasExperimentalCondition ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass :ExperimentalCondition
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasExperimentalData ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass :ExperimentalData
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasSetting ;
                              owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                              owl:onClass :Setting
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :modifiedAtTime ;
                              owl:someValuesFrom xsd:dateTime
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :description ;
                              owl:allValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :name ;
                              owl:allValuesFrom xsd:string
                            ] ;
            <http://ontologydesignpatterns.org/opla#isNativeTo> :Study ;
            rdfs:comment "Experiment is an entity which describes the collective activity conducted in a science lab." ;
            rdfs:label "Experiment"@en .


###  https://w3id.org/reproduceme/core#ExperimentEnvironmentSetup
:ExperimentEnvironmentSetup rdf:type owl:Class ;
                            rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                                              owl:someValuesFrom :Experiment
                                            ] ;
                            <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                            rdfs:comment "A set of instructions to setup the environment for experiment."@en ;
                            rdfs:label "Experiment Environment Setup"@en .


###  https://w3id.org/reproduceme/core#ExperimentMaterial
:ExperimentMaterial rdf:type owl:Class ;
                    rdfs:subClassOf <http://purl.org/net/p-plan#Variable> ,
                                    <http://www.w3.org/ns/prov#Entity> ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :description ;
                                      owl:allValuesFrom xsd:string
                                    ] ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty :name ;
                                      owl:allValuesFrom xsd:string
                                    ] ;
                    <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                    rdfs:comment "The materials or things used in an experiment."@en ;
                    rdfs:label "Experiment Material"@en .


###  https://w3id.org/reproduceme/core#ExperimentMaterialPreparation
:ExperimentMaterialPreparation rdf:type owl:Class ;
                               rdfs:subClassOf <http://purl.org/net/p-plan#Plan> ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty <http://purl.org/net/p-plan#isSubPlanOfPlan> ;
                                                 owl:someValuesFrom :Experiment
                                               ] ;
                               <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                               rdfs:comment "A procedure to prepare experiment materials."@en ;
                               rdfs:label "Experiment Material Preparation"@en .


###  https://w3id.org/reproduceme/core#ExperimentSetup
:ExperimentSetup rdf:type owl:Class ;
                 rdfs:subClassOf <http://purl.org/net/p-plan#Step> ;
                 <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                 rdfs:comment "To setup the execution environment of an experiment."@en ;
                 rdfs:label "Experiment Setup"@en .


###  https://w3id.org/reproduceme/core#ExperimentalCondition
:ExperimentalCondition rdf:type owl:Class ;
                       rdfs:subClassOf :Setting ;
                       rdfs:comment "The attributes or the conditions while an experiment is performed."@en ;
                       rdfs:label "Experimental Condition"@en .


###  https://w3id.org/reproduceme/core#ExperimentalData
:ExperimentalData rdf:type owl:Class ;
                  rdfs:subClassOf <http://purl.org/net/p-plan#Variable> ,
                                  <http://www.w3.org/ns/prov#Entity> ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :storedAt ;
                                    owl:someValuesFrom <http://www.w3.org/ns/prov#Location>
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :description ;
                                    owl:allValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :name ;
                                    owl:allValuesFrom xsd:string
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :doi ;
                                    owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onDataRange xsd:string
                                  ] ;
                  <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
                  rdfs:comment "Data used in an experiment."@en ;
                  rdfs:label "Experimental Data"@en .


###  https://w3id.org/reproduceme/core#Experimenter
:Experimenter rdf:type owl:Class ;
              rdfs:subClassOf <http://www.w3.org/ns/prov#Person> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :address ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :name ;
                                owl:allValuesFrom xsd:string
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :ORCID ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onDataRange xsd:string
                              ] ;
              <http://ontologydesignpatterns.org/opla#isNativeTo> <http://www.w3.org/ns/prov#Agent> ;
              rdfs:comment "Experimenter is a person who is the owner of the Experiment and has its responsibility." ;
              rdfs:label "Experimenter"@en .


###  https://w3id.org/reproduceme/core#File
:File rdf:type owl:Class ;
      rdfs:subClassOf :ExperimentalData ;
      rdfs:comment "A storage to store data. For example, a computer file."@en ;
      rdfs:label "File"@en .


###  https://w3id.org/reproduceme/core#FinalResult
:FinalResult rdf:type owl:Class ;
             rdfs:subClassOf :Result ;
             rdfs:comment "It is the final result that is generated in an experiment which is eventually used in a publication."@en ;
             rdfs:label "Final Result"@en .


###  https://w3id.org/reproduceme/core#FinalStep
:FinalStep rdf:type owl:Class ;
           rdfs:subClassOf <http://purl.org/net/p-plan#Step> ;
           <http://ontologydesignpatterns.org/opla#isNativeTo> :Experiment ;
           rdfs:comment "The step that is performed at the end of an experiment."@en ;
           rdfs:label "Final Step"@en .


